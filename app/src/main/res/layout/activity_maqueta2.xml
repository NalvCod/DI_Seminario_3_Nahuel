<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Maqueta2">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/barraPrincipal"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/design_default_color_secondary_variant"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/barraIcono1"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageButton"
            android:layout_marginEnd="15dp"
            app:layout_constraintTop_toTopOf="parent"
            tools:srcCompat="@drawable/iconomenu" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="Chat grupal"
            android:textSize="20dp"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="15dp">

        </TextView>


        <ImageView
            android:id="@+id/imageButton"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="15dp"
            app:layout_constraintTop_toTopOf="parent"
            tools:srcCompat="@drawable/iconousuario" />



    </androidx.constraintlayout.widget.ConstraintLayout>
        <ImageView
            android:id="@+id/burbuja1Icono1"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_constraintEnd_toStartOf="@id/burbuja1Titulo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/burbuja1Titulo"
            tools:srcCompat="@tools:sample/avatars"
            android:layout_marginStart="10dp"/>

        <TextView
            android:id="@+id/burbuja1Titulo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@+id/burbuja1Icono1"
            app:layout_constraintTop_toBottomOf="@id/barraPrincipal"
            android:text="FrikiDemente420"
            android:textSize="25dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="15dp">
        </TextView>

    <ImageView
        android:id="@+id/burbuja1Icono2"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/burbuja1Titulo"
        android:layout_marginEnd="15dp"
        tools:srcCompat="@drawable/iconopuntos" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/burbuja1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/burbuja1Icono1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:cardBackgroundColor="@color/design_default_color_secondary_variant"
        android:layout_margin="20dp"
        app:cardCornerRadius="20dp">

        <TextView
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:padding="20dp"
            android:text="AAAAAA"
            />

    </com.google.android.material.card.MaterialCardView>

    <ImageView
        android:id="@+id/burbuja2Icono1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintEnd_toStartOf="@id/burbuja2Titulo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/burbuja2Titulo"
        tools:srcCompat="@tools:sample/avatars"
        android:layout_marginStart="10dp"/>

    <TextView
        android:id="@+id/burbuja2Titulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@+id/burbuja2Icono1"
        app:layout_constraintTop_toBottomOf="@id/burbuja1"
        android:text="Narcisista32"
        android:textSize="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="15dp">
    </TextView>

    <ImageView
        android:id="@+id/burbuja2Icono2"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/burbuja2Titulo"
        android:layout_marginEnd="15dp"
        tools:srcCompat="@drawable/iconopuntos" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/burbuja2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/burbuja2Icono1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:cardBackgroundColor="@color/design_default_color_secondary_variant"
        android:layout_margin="20dp"
        app:cardCornerRadius="20dp">

        <TextView
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:padding="20dp"
            android:text="AAAAAA"
            />
    </com.google.android.material.card.MaterialCardView>

    <ImageView
        android:id="@+id/burbuja3Icono1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintEnd_toStartOf="@id/burbuja3Titulo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/burbuja3Titulo"
        tools:srcCompat="@tools:sample/avatars"
        android:layout_marginStart="10dp"/>

    <TextView
        android:id="@+id/burbuja3Titulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@+id/burbuja3Icono1"
        app:layout_constraintTop_toBottomOf="@id/burbuja2"
        android:text="tuFurry69"
        android:textSize="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="15dp">
    </TextView>

    <ImageView
        android:id="@+id/burbuja3Icono2"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/burbuja3Titulo"
        android:layout_marginEnd="15dp"
        tools:srcCompat="@drawable/iconopuntos" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/burbuja3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/burbuja3Icono1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:cardBackgroundColor="@color/design_default_color_secondary_variant"
        android:layout_margin="20dp"
        app:cardCornerRadius="20dp">

        <TextView
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:padding="20dp"
            android:text="AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA


AAAAAAA"
            />
    </com.google.android.material.card.MaterialCardView>











</androidx.constraintlayout.widget.ConstraintLayout>